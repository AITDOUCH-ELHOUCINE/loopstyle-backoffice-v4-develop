{
  "name": "LoopStyle-API",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "test": "npm run test:prefix ./modules/*/tests/*.test.js",
    "test:prefix": "mocha --exit --recursive ./helpers/test-hooks.js",
    "pretest": "npm run lint",
    "test:watch": "nodemon --watch modules/**/*.js --exec \"npm test\"",
    "format:write": "prettier --write **/*.{json,md} !package.json",
    "format:test": "prettier **/*.{json,md} --list-different",
    "start": "nodemon --inspect .",
    "start:db": "mongod --dbpath db",
    "start:db:dev": "mongod --dbpath db --config db/mongodb.conf",
    "start:dev": "pm2 stop loopstyle-backend-dev-v1 && pm2 delete loopstyle-backend-dev-v1 &&  pm2 start ecosystem.config.js --env development && pm2 log",
    "start:staging": "pm2 stop loopstyle-backend-staging-v1 && pm2 delete loopstyle-backend-staging-v1 &&  pm2 start ecosystem.config.js --env staging && pm2 log",
    "start:prod": "pm2 stop loopstyle-backend-prod-v1 && pm2 delete loopstyle-backend-prod-v1 &&  pm2 start ecosystem.config.js --env production && pm2 log",
    "stop": "pm2 stop all",
    "seed": "node seeds/seed.js",
    "restart": "pm2 restart all",
    "lint": "eslint . --resolve-plugins-relative-to .",
    "watch": "watch 'npm run lint && npm test' -d -u -p /^node_modules/ .",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch",
    "webdash": "webdash serve",
    "generate:module": "plop module",
    "generate:certs": "scripts/generate-ssl-certs.sh",
    "postinstall": "node ./scripts/install-dependencies.js",
    "npm:update": "./scripts/update.js --cmd=npm-update",
    "npm:audit": "./scripts/update.js --cmd=npm-audit",
    "npm:audit:fix": "./scripts/update.js --cmd=npm-audit-fix",
    "git:pull": "./scripts/update.js --cmd=git-pull --only=git",
    "git:commit": "./scripts/update.js --cmd=git-commit --only=git",
    "git:status": "./scripts/update.js --cmd=git-status --only=git",
    "git:revert": "./scripts/update.js --cmd=git-revert --only=git"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@api.video/nodejs-client": "2.3.1",
    "@googlemaps/google-maps-services-js": "^3.1.13",
    "@mailchimp/mailchimp_marketing": "^3.0.74",
    "@nicokaiser/passport-apple": "^0.2.1",
    "@sendgrid/mail": "^7.2.1",
    "@sentry/node": "^5.30.0",
    "@sentry/tracing": "^6.5.1",
    "@socket.io/redis-adapter": "^7.2.0",
    "@turf/turf": "^5.1.6",
    "agenda": "^3.1.0",
    "ajv": "^6.12.2",
    "ajv-errors": "^1.0.1",
    "apicache": "^1.6.3",
    "axios": "^0.21.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "express-session": "1.18.0",
    "file-stream-rotator": "^0.5.7",
    "generate-password": "^1.5.1",
    "glob": "^7.1.6",
    "handlebars-intl": "^1.1.2",
    "helmet": "^3.23.3",
    "html-pdf": "^2.2.0",
    "i18next": "^19.9.2",
    "i18next-express-middleware": "^2.0.0",
    "i18next-node-fs-backend": "^2.1.3",
    "install": "^0.13.0",
    "json2csv": "^6.0.0-alpha.2",
    "lodash": "^4.17.15",
    "lru-cache": "^10.0.1",
    "method-override": "^3.0.0",
    "mime": "^2.4.6",
    "minimatch": "^3.0.4",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "mongodb": "^6.16.0",
    "mongoose": "^5.13.23",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.2.0",
    "nodemailer": "^6.4.10",
    "npm2": "^2.0.0",
    "nunjucks": "^3.2.1",
    "onesignal-node": "^3.4.0",
    "owasp-password-strength-test": "^1.3.0",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "path-to-regexp": "0.1.7",
    "payxpert": "^1.0.4",
    "react-native": "^0.79.1",
    "redis": "^4.3.1",
    "sharp": "^0.26.3",
    "socket.io": "^4.6.0",
    "stream": "^0.0.2",
    "stripe": "^10.15.0",
    "twilio": "^3.47.0",
    "uuid": "^8.3.2",
    "validator": "^13.1.1",
    "xlsx": "^0.16.9"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "./app",
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "chai": "^4.2.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "handlebars": "^4.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "ora": "^4.0.4",
    "plop": "^2.7.1",
    "pm2": "^4.4.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "release-it": "^13.6.4",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "watch": "^1.0.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@packages": "packages",
    "@helpers": "helpers",
    "@config": "config",
    "@modules": "modules"
  }
}
